<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:grid="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@+id/bRandom"
    grid:columnCount="3"
    grid:rowCount="3">

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b1"
        android:text="1"
        grid:layout_column="0"
        grid:layout_columnWeight="1"
        grid:layout_row="0"
        grid:layout_rowWeight="1" />

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b2"
        android:text="2"
        grid:layout_column="1"
        grid:layout_columnWeight="1"
        grid:layout_row="0"
        grid:layout_rowWeight="1" />

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b3"
        android:text="3"
        grid:layout_column="2"
        grid:layout_columnWeight="1"
        grid:layout_row="0"
        grid:layout_rowWeight="1" />

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b4"
        android:text="4"
        grid:layout_column="0"
        grid:layout_columnWeight="1"
        grid:layout_row="1"
        grid:layout_rowWeight="1" />

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b5"
        android:text="5"
        grid:layout_column="1"
        grid:layout_columnWeight="1"
        grid:layout_row="1"
        grid:layout_rowWeight="1" />

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b6"
        android:text="6"
        grid:layout_column="2"
        grid:layout_columnWeight="1"
        grid:layout_row="1"
        grid:layout_rowWeight="1" />

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b7"
        android:text="7"
        grid:layout_column="0"
        grid:layout_columnWeight="1"
        grid:layout_row="2"
        grid:layout_rowWeight="1" />

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b8"
        android:text="8"
        grid:layout_column="1"
        grid:layout_columnWeight="1"
        grid:layout_row="2"
        grid:layout_rowWeight="1" />

    <com.lalagrass.slidepuzzle.MyClass.GridLayoutButton
        android:id="@+id/b9"
        android:visibility="invisible"
        grid:layout_column="2"
        grid:layout_columnWeight="1"
        grid:layout_row="2"
        grid:layout_rowWeight="1" />
</merge>
